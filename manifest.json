{
  "system": "Wix",
  "additional_parameters": {
    "wix-site-id": {},
    "base_url": {}
  },
  "auth": "api_key",
  "datatypes": {
    "contacts": {
      "template": "templates/contacts.json"
    },
    "inventory": {
      "parameters": {
        "payload_property": "inventoryItem"
      },
      "template": "templates/products.json"
    },
    "members": {
      "template": "templates/contacts.json"
    },
    "orders": {
      "parameters": {
        "payload_property": "order"
      },
      "template": "templates/products.json"
    },
    "products": {
      "parameters": {
        "payload_property": "product"
      },
      "template": "templates/products.json"
    },
    "currencies": {
      "template": "templates/contacts.json"
    }
  },
  "system-template": "templates/system.json"
}